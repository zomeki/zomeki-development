<div class="contentGpCategoryCategoryType">
  <%- unless @docs.empty? -%>
  <div class="docs">
    <ul class="feed">
      <li><%= link_to 'RSS' , "index.rss" , :class => 'feedRss', :title => 'RSSフィードを取得' %></li>
      <li><%= link_to 'Atom', "index.atom", :class => 'feedAtom', :title => 'Atomフィードを取得' %></li>
    </ul>
    <ul>
      <%- @docs.each do |doc| -%>
      <li><%= render 'gp_category/public/shared/doc', doc: doc, doc_style: @content.category_type_doc_style, date_style: @content.date_style %></li>
      <%- end -%>
    </ul>
  </div>
  <%- end -%>

  <%= paginate @docs %>
</div>
